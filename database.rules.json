{
	"rules": {
		"users": {
			".read": true,
				"$uid": {
					".write": "auth.uid === $uid"
				}
		},
			"followers": {
				"$followedUid": {
					"$followerUid": {
						".read": "auth.uid === $followerUid",
						".write": "auth.uid === $followerUid"
					}		
				}
			},
			"rooms":{
				".read": true
			},
			"messages": {
				".read": true
			},
			"roomList":{
				"$uid":{
					".read":"auth.uid === $uid",
					".indexOn": ".value" 
				},
				".write":true
			},
			"uncheked":{
				".read":true,
				".write":true
			}
	}
}

