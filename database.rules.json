{
	"rules": {
		"users": {
			".read": true,
				"$uid": {
					".write": "auth.uid === $uid"
				}
		},
			"followers": {
				"$followedUid": {
					"$followerUid": {
						".read": "auth.uid === $followerUid",
						".write": "auth.uid === $followerUid"
					}		
				}
			},
			"rooms":{
				".read": true
			},
			"messages": {
        "$roomname":{
          ".read":true
        },
				".read": false,
				".write":true
			},
      "roomList":{
        "$uid":{
          ".read":"auth.uid === $uid",
          ".indexOn": ".value" 
        },
        ".write":true
      },
        "uncheked":{
          ".read":true,
            ".write":true
        }
	}
}
